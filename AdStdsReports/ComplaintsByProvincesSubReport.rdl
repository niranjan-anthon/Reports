      <DataSourceReference>AdStdsCMS</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>002ee0c8-a25f-4780-a7ed-1da0cd7c38b2</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>AdStdsCMS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@start_date">
            <Value>=Parameters!start_date.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@end_date">
            <Value>=Parameters!end_date.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        ISNULL(COUNT(b.complaint_id), 0) AS complaints_count, a.province_name
FROM            Provinces AS a LEFT OUTER JOIN
                         Complaints AS b ON a.province_id = b.province_id AND b.created_date &gt;= @start_date AND b.created_date &lt;= @end_date  AND (b.daac_complaint = 'FALSE' OR b.daac_complaint IS NULL)
WHERE        (a.country_id =
                             (SELECT        country_id
                               FROM            Countries
                               WHERE        (country_name = 'Canada')))
GROUP BY a.province_name
ORDER BY a.province_name</CommandText>
      </Query>
      <Fields>
        <Field Name="complaints_count">
          <DataField>complaints_count</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="province_name">
          <DataField>province_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
