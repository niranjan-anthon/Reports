  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>AdStdsCMS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@start_date">
            <Value>=Parameters!start_date.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@end_date">
            <Value>=Parameters!end_date.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@province_id">
            <Value>=Parameters!province_id.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@campaign_name">
            <Value>=Parameters!campaign_name.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@advertiser_name">
            <Value>=Parameters!advertiser_name.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        b.company_name, a.campaign_name, ISNULL(a.assigned_complaints, 0) AS complaints_count
FROM            Cases AS a LEFT OUTER JOIN
                         Companies AS b ON a.advertiser_id = b.company_id AND a.created_date &gt;= @start_date AND a.created_date &lt;= @end_date 
WHERE (a.province_id = @province_id OR @province_id IS NULL) AND (a.campaign_name LIKE '%' + @campaign_name + '%' AND b.company_name LIKE '%' + @advertiser_name + '%')
GROUP BY b.company_name, a.campaign_name, a.assigned_complaints
ORDER BY b.company_name</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="complaints_count">
          <DataField>complaints_count</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="campaign_name">
          <DataField>campaign_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="company_name">
          <DataField>company_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
