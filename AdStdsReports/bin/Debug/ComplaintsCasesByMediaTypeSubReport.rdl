      <DataSourceReference>AdStdsCMS</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>60880cee-cba4-4d4f-9933-68c0659fddf3</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>AdStdsCMS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@start_date">
            <Value>=Parameters!start_date.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@end_date">
            <Value>=Parameters!end_date.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT       count(Cases.case_id) AS case_count, ComplaintMediaTypes.complaint_media_type
FROM            Cases LEFT OUTER JOIN
                         ComplaintMediaTypes ON Cases.complaint_media_type_id = ComplaintMediaTypes.complaint_media_type_id
WHERE        (Cases.created_date &gt;= @start_date) AND (Cases.created_date &lt;= @end_date)
GROUP BY ComplaintMediaTypes.complaint_media_type</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="case_count">
          <DataField>case_count</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="complaint_media_type">
          <DataField>complaint_media_type</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
