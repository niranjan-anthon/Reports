  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>AdStdsCMS</DataSourceName>
        <CommandText>SELECT        COUNT(ISNULL(Cases.case_id, 0)) AS case_count, CaseStatuses.case_status, ComplaintIndustries.industry + '/' + ComplaintTags.complaint_tag AS industry_tags
FROM            ComplaintCaseTagAssignments RIGHT OUTER JOIN
                         ComplaintTags ON ComplaintCaseTagAssignments.complaint_tag_id = ComplaintTags.complaint_tag_id LEFT OUTER JOIN
                         Cases ON ComplaintCaseTagAssignments.case_id = Cases.case_id RIGHT OUTER JOIN
                         ComplaintIndustries LEFT OUTER JOIN
                         ComplaintCaseIndustryAssignments ON ComplaintCaseIndustryAssignments.complaint_industry_id = ComplaintIndustries.complaint_industry_id ON 
                         Cases.case_id = ComplaintCaseIndustryAssignments.case_id LEFT OUTER JOIN
                         CaseStatuses ON CaseStatuses.case_status_id = Cases.case_status_id
GROUP BY Cases.case_id, CaseStatuses.case_status, ComplaintIndustries.industry, ComplaintTags.complaint_tag</CommandText>
      </Query>
      <Fields>
        <Field Name="case_count">
          <DataField>case_count</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="case_status">
          <DataField>case_status</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="industry_tags">
          <DataField>industry_tags</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
