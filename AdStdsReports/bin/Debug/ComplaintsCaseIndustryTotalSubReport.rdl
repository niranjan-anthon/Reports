  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>AdStdsCMS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@start_date">
            <Value>=Parameters!start_date.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@end_date">
            <Value>=Parameters!end_date.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@province_id">
            <Value>=Parameters!province_id.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        COUNT(Cases.case_id) AS case_count, ISNULL(ComplaintIndustries.industry, 'Not Applicable') AS industry, Provinces.province_name
FROM            ComplaintIndustries LEFT OUTER JOIN
                         ComplaintCaseIndustryAssignments ON ComplaintCaseIndustryAssignments.complaint_industry_id = ComplaintIndustries.complaint_industry_id LEFT OUTER JOIN
                         Cases ON Cases.case_id = ComplaintCaseIndustryAssignments.case_id AND Cases.created_date &gt;= @start_date AND Cases.created_date &lt;= @end_date
LEFT OUTER JOIN Provinces ON Cases.province_id = Provinces.province_id
WHERE        (Cases.province_id = @province_id) OR
                         (@province_id IS NULL)
GROUP BY ComplaintIndustries.industry, Provinces.province_name
ORDER BY industry</CommandText>
      </Query>
      <Fields>
        <Field Name="case_count">
          <DataField>case_count</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="industry">
          <DataField>industry</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="province_name">
          <DataField>province_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
