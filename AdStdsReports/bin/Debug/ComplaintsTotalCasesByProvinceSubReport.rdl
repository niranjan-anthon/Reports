  <DataSources>
    <DataSource Name="AdStdsCMS">
      <DataSourceReference>AdStdsCMS</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>5fcc94ab-feb1-41b5-8a73-8b41d1983d3b</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>AdStdsCMS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@start_date">
            <Value>=Parameters!start_date.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@end_date">
            <Value>=Parameters!end_date.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        ISNULL(COUNT(b.case_id),0) AS cases_count, a.province_name
FROM            Provinces AS a LEFT OUTER JOIN
                         Cases AS b ON a.province_id = b.province_id AND b.created_date &gt;= @start_date AND b.created_date &lt;= @end_date
WHERE        (a.country_id =
                             (SELECT        country_id
                               FROM            Countries
                               WHERE        (country_name = 'Canada')))
GROUP BY a.province_name
ORDER BY a.province_name</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="cases_count">
          <DataField>cases_count</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="province_name">
          <DataField>province_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
