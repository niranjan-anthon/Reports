  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>AdStdsCMS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@start_date">
            <Value>=Parameters!start_date.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@end_date">
            <Value>=Parameters!end_date.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@province_id">
            <Value>=Parameters!province_id.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        COUNT(Cases.case_id) AS case_count, CaseStatuses.case_status, ComplaintMediaTypes.complaint_media_type
FROM      CaseStatuses       
LEFT OUTER JOIN
                        Cases  ON Cases.case_status_id = CaseStatuses.case_status_id AND (Cases.created_date &gt;= @start_date) AND (Cases.created_date &lt;= @end_date) AND (Cases.province_id = @province_id OR @province_id IS NULL)
						LEFT OUTER JOIN ComplaintMediaTypes ON Cases.complaint_media_type_id = ComplaintMediaTypes.complaint_media_type_id
GROUP BY CaseStatuses.case_status, ComplaintMediaTypes.complaint_media_type</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="case_count">
          <DataField>case_count</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="case_status">
          <DataField>case_status</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="complaint_media_type">
          <DataField>complaint_media_type</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
