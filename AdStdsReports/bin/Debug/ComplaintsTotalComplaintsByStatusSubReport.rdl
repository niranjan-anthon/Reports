    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>AdStdsCMS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@start_date">
            <Value>=Parameters!start_date.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@end_date">
            <Value>=Parameters!end_date.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        COUNT(ISNULL(Complaints.complaint_id, 0)) AS complaint_count, ComplaintStatuses.complaint_status
FROM            ComplaintStatuses LEFT OUTER JOIN
                         Complaints ON Complaints.complaint_status_id = ComplaintStatuses.complaint_status_id AND ComplaintStatuses.daac &lt;&gt; 'true' AND Complaints.created_date &gt;= @start_date AND Complaints.created_date &lt;= @end_date AND 
                         (Complaints.daac_complaint = 'FALSE' OR
                         Complaints.daac_complaint IS NULL)
GROUP BY ComplaintStatuses.complaint_status</CommandText>
      </Query>
      <Fields>
        <Field Name="complaint_count">
          <DataField>complaint_count</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="complaint_status">
          <DataField>complaint_status</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
