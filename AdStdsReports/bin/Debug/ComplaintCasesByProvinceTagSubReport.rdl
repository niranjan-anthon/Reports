  <DataSources>
    <DataSource Name="AdStdsCMS">
      <DataSourceReference>AdStdsCMS</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>8e5bea0b-4c91-45e5-b542-f9783cb4f9ed</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>AdStdsCMS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@start_date">
            <Value>=Parameters!start_date.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@end_date">
            <Value>=Parameters!end_date.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@province_id">
            <Value>=Parameters!province_id.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        ct.complaint_tag, p.province_name, COUNT(c.case_id) AS case_count
FROM ComplaintTags AS ct
LEFT OUTER JOIN ComplaintCaseTagAssignments AS cct ON  ct.complaint_tag_id = cct.complaint_tag_id
LEFT OUTER JOIN Cases AS c ON c.case_id = cct.case_id AND c.created_date &gt;= @start_date AND c.created_date &lt;= @end_date AND (c.province_id = @province_id OR @province_id IS NULL)
LEFT OUTER JOIN Provinces AS p ON c.province_id = p.province_id
WHERE        (p.province_name IS NOT NULL)
GROUP BY ct.complaint_tag, p.province_name
ORDER BY p.province_name, ct.complaint_tag</CommandText>
      </Query>
      <Fields>
        <Field Name="complaint_tag">
          <DataField>complaint_tag</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="province_name">
          <DataField>province_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="case_count">
          <DataField>case_count</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
