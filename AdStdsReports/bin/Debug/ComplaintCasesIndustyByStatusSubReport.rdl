  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>AdStdsCMS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@province_id">
            <Value>=Parameters!province_id.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        COUNT(Cases.case_id) AS case_count, CaseStatuses.case_status, ComplaintIndustries.industry
FROM       ComplaintIndustries
LEFT OUTER JOIN ComplaintCaseIndustryAssignments ON ComplaintCaseIndustryAssignments.complaint_industry_id = ComplaintIndustries.complaint_industry_id
LEFT OUTER JOIN Cases ON Cases.case_id = ComplaintCaseIndustryAssignments.case_id AND (Cases.province_id = @province_id OR @province_id IS NULL)
LEFT OUTER JOIN CaseStatuses ON CaseStatuses.case_status_id = Cases.case_status_id             
GROUP BY CaseStatuses.case_status, ComplaintIndustries.industry</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="case_count">
          <DataField>case_count</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="case_status">
          <DataField>case_status</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="industry">
          <DataField>industry</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
