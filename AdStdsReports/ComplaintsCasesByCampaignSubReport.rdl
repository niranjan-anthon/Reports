  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>AdStdsCMS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@start_date">
            <Value>=Parameters!start_date.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@campaign_name">
            <Value>=Parameters!campaign_name.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@end_date">
            <Value>=Parameters!end_date.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        COUNT(ISNULL(a.case_id,0)) AS cases_count, a.campaign_name
FROM            Cases AS a 
WHERE        (a.created_date &gt;= @start_date) AND (a.created_date &lt;= @end_date)
AND a.campaign_name LIKE '%' + @campaign_name + '%'
GROUP BY a.campaign_name
ORDER BY a.campaign_name</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="cases_count">
          <DataField>cases_count</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="campaign_name">
          <DataField>campaign_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
