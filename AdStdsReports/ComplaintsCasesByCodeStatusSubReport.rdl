  <DataSources>
    <DataSource Name="AdStdsCMS">
      <DataSourceReference>AdStdsCMS</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>00290d62-34d9-4d9b-b8f2-621ff2a7137f</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>AdStdsCMS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@start_date">
            <Value>=Parameters!start_date.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@end_date">
            <Value>=Parameters!end_date.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@province_id">
            <Value>=Parameters!province_id.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        COUNT(Cases.case_id) AS case_count, CaseStatuses.case_status, ComplaintCodes.category, ComplaintCodes.code, CAST(ComplaintCodes.code_description AS NVARCHAR(MAX)) AS code_description
FROM   CaseStatuses
LEFT OUTER JOIN Cases ON CaseStatuses.case_status_id = Cases.case_status_id 
LEFT OUTER JOIN  ComplaintCaseCodeAssignments ON Cases.case_id = ComplaintCaseCodeAssignments.case_id AND (Cases.created_date &gt;= @start_date) AND (Cases.created_date &lt;= @end_date) AND (Cases.province_id = @province_id OR @province_id IS NULL)
RIGHT OUTER JOIN ComplaintCodes     ON ComplaintCaseCodeAssignments.complaint_code_id = ComplaintCodes.complaint_code_id 
WHERE ComplaintCodes.active = 'TRUE'
GROUP BY ComplaintCodes.category, ComplaintCodes.code, CAST(ComplaintCodes.code_description AS NVARCHAR(MAX)), CaseStatuses.case_status 
ORDER BY ComplaintCodes.category, ComplaintCodes.code, CAST(ComplaintCodes.code_description AS NVARCHAR(MAX)), CaseStatuses.case_status </CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
