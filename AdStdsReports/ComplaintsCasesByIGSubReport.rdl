    <DataSource Name="AdStdsCMS">
      <DataSourceReference>AdStdsCMS</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>f1d22441-9b50-485f-b1c7-854093e64e73</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>AdStdsCMS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@start_date">
            <Value>=Parameters!start_date.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@end_date">
            <Value>=Parameters!end_date.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@province_id">
            <Value>=Parameters!province_id.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        COUNT(Cases.case_id) AS case_count, ISNULL(ComplaintClauses.clause_code, 'Not Applicable') AS clause_code, CAST(ComplaintClauses.clause_description AS NVARCHAR(MAX)) AS clause_description
FROM       ComplaintClauses 
LEFT OUTER JOIN ComplaintCaseClauseAssignments ON ComplaintCaseClauseAssignments.complaint_clause_id = ComplaintClauses.complaint_clause_id
LEFT OUTER JOIN Cases ON Cases.case_id = ComplaintCaseClauseAssignments.case_id  AND (Cases.created_date &gt;= @start_date) AND (Cases.created_date &lt;= @end_date) AND (Cases.province_id = @province_id OR @province_id IS NULL)
LEFT OUTER JOIN CaseStatuses ON CaseStatuses.case_status_id = Cases.case_status_id             
GROUP BY ComplaintClauses.clause_code, CAST(ComplaintClauses.clause_description AS NVARCHAR(MAX))
ORDER BY ComplaintClauses.clause_code</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="case_count">
          <DataField>case_count</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="clause_code">
          <DataField>clause_code</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="clause_description">
          <DataField>clause_description</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
